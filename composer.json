{
  "name": "jamin87/laravel-sitesettings",
  "description": "Persistant site settings for Laravel 5.4",
  "keywords": [
    "module",
    "laravel"
  ],
  "authors": [
    {
      "name": "ben",
      "email": "benjamin.wibrew@gmail.com"
    }
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "Illuminate/Support": "^5.4",
    "illuminate/database": "^5.4"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.6",
    "mockery/mockery": "^0.9.9",
    "orchestra/database": "^3.4",
    "orchestra/testbench": "~3.4",
    "phpunit/phpunit": "^6.2",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Jamin87\\SiteSettings\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jamin87\\SiteSettings\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : [
      "vendor/bin/phpcs",
      "vendor/bin/phpunit"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
